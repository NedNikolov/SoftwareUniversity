def plot_function(f, x_min = -10, x_max = 10, n_values = 2000):
    plt.gca().set_aspect("equal")
    x = np.linspace(x_min, x_max, n_values)
    y = f(x)
    plt.plot(x, y)
    

plt.show()
plot_function(lambda x: np.sqrt(1 - x**2), -1, 1)
plot_function(lambda x: -np.sqrt(1 - x**2), -1, 1)


